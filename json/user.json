{
    "id": {
        "type": "Sequelize.INTEGER",
        "autoIncrement": true,
        "allowNull": false,
        "primaryKey": true
    },
    "first_name": {
        "type": "Sequelize.STRING",
        "allowNull": false,
        "require": true
    },
    "last_name": {
        "type": "Sequelize.STRING",
        "allowNull": false
    },
    "email": {
        "type": "Sequelize.STRING",
        "allowNull": false,
        "unique": true,
        "require": true
    },
    "phone": {
        "type": "Sequelize.STRING",
        "allowNull": false
    },
    "password": {
        "type": "Sequelize.STRING",
        "allowNull": false,
        "require": true
    }
}